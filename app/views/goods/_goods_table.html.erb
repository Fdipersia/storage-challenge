<table>
  <tr>
    <th>Consignment ID</th>
    <th>Name</th>
    <th>Type</th>
    <th>Source</th>
    <th>Destination</th>
    <th>Entry at</th>
    <th>Exit at</th>
  </tr>
  <% goods.each do |good| %>
    <tr>
      <td><%= good.consignment_id %></td>
      <td><%= good.name %></td>
      <td><%= good.category %></td>
      <td><%= good.source %></td>
      <td><%= good.destination %></td>
      <td><%= good.entry_at %></td>
      <td><%= good.exit_at %></td>
    </tr>
  <% end %>
</table>